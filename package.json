{
  "name": "smsup",
  "version": "0.0.0",
  "private": true,
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client": "babel-node bin/start-client.js",
    "server": "./node_modules/.bin/babel-node ./bin/start-server.js",
    "build": "cd client ; npm run build ; cd ..",
    "test": "jest ; ./node_modules/.bin/cucumber.js --compiler es6:babel-core/register"
  },
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^4.6.8",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "babel-jest": "^17.0.2",
    "babel-polyfill": "^6.16.0",
    "cucumber": "^2.0.0-rc.5",
    "dotenv": "^2.0.0",
    "nock": "^9.0.2",
    "supertest": "^2.0.1"
  },
  "jest": {
    "testPathDirs": [
      "server",
      "client"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
